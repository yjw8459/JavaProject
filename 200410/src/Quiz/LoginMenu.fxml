               </VBox.margin>
            </TextField>
            <Button mnemonicParsing="false" prefHeight="28.0" prefWidth="138.0" text="Sign in">
               <VBox.margin>
                  <Insets top="8.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" prefHeight="28.0" prefWidth="138.0" text="Sign up">
               <VBox.margin>
                  <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
