<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<BorderPane prefHeight="400.0" prefWidth="600.0" 
xmlns="http://javafx.com/javafx/8" 
xmlns:fx="http://javafx.com/fxml/1"
fx:controller="Webpage.Controller">
   <top>
      <Label text="Jin Homepage" BorderPane.alignment="CENTER">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
   </top>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <left>
      <VBox prefHeight="344.0" prefWidth="131.0" spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="loginBtn" mnemonicParsing="false" onAction="#LoginView" prefHeight="30.0" prefWidth="100.0" text="Login" />
            <Button fx:id="membershipBtn" mnemonicParsing="false" onAction="#MembershipView" prefHeight="30.0" prefWidth="100.0" text="Membership" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </left>
</BorderPane>
