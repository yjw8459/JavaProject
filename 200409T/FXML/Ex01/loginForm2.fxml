<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<HBox prefHeight="77.0" prefWidth="195.0" spacing="20.0" 
xmlns="http://javafx.com/javafx/8" 
xmlns:fx="http://javafx.com/fxml/1"
fx:controller="FXML.Ex01.Controller">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0">
         <children>
            <TextField fx:id="idTxt" promptText="ID 입력" />
            <TextField fx:id="pwTxt" promptText="PW 입력" />
         </children>
      </VBox>
      <Button fx:id="okBtn" mnemonicParsing="false" prefHeight="54.0" prefWidth="55.0" text="OK" />
   </children>
</HBox>
